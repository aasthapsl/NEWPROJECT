<div class="page-width page-width--narrow section-{{ section.id }}-padding">
  <h1 class="main-page-title page-title h0">
    {{ page.title | escape }}
  </h1>
  <div class="rte">
    {{ page.content }}
  </div>
</div>
  {% schema %}
  {
  "name": "Custom Page",
  "tag": "section",
  "class": "section",
  "settings": [
    {
        "type":"text",
        "id":"custom-class",
        "label":"CSS class"
    },     
    {
        "type":"text",
        "id":"custom-id",
        "label":"CSS ID"
    }
  ],
  "presets": [
    {
      "name": "Custom Page"
    }
  ]
  }
  {% endschema %}